<?xml version="1.0"?>
<package>
    <name>mcr_trajectory_time_parameterizer</name>
    <version>0.1.0</version>
    <description>
        This package provides the functionality to time parameterize
        the given trajectory which only containts joint positions.
    </description>

    <maintainer email="shehzad.ahmed@smail.inf.h-brs.de">Shehzad Ahmed</maintainer>

    <license>GPLv3</license>

    <buildtool_depend>catkin</buildtool_depend>

    <build_depend>moveit_core</build_depend>
    <build_depend>moveit_ros_occupancy_map_monitor</build_depend>
    <build_depend>moveit_ros_planning</build_depend>
    <build_depend>moveit_ros_planning_interface</build_depend>
    <build_depend>roscpp</build_depend>
    <build_depend>geometric_shapes</build_depend>
    <build_depend>geometry_msgs</build_depend>
    <build_depend>std_msgs</build_depend>
    <build_depend>trajectory_msgs</build_depend>
    <build_depend>roslint</build_depend>

    <run_depend>moveit_core</run_depend>
    <run_depend>moveit_ros_occupancy_map_monitor</run_depend>
    <run_depend>moveit_ros_planning</run_depend>
    <run_depend>moveit_ros_planning_interface</run_depend>
    <run_depend>roscpp</run_depend>
    <run_depend>geometric_shapes</run_depend>
    <run_depend>geometry_msgs</run_depend>
    <run_depend>trajectory_msgs</run_depend>
    <run_depend>std_msgs</run_depend>
</package>
